# The Project_Contributors and Readers azuread_group need to be created before this terraform ini can be executed. Likelly in some 
# form of a CoreInfra project initialisation terraform statement like it is the case for this project.

data "azuread_group" "Project_Contributors_group" {
  name = "${local.prefix}_${var.project}-Contributors"
}

data "azuread_group" "Project_Readers_group" {
  name = "${local.prefix}_${var.project}-Readers"
}